	<xs:complexType name="VoucherUploadRq_Type">
		<xs:annotation>
			<xs:documentation>A collection of voucher records and is used to activate, deactivate vouchers within SADAD system. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Timestamp"/>
			<xs:element ref="VoucherRec" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VoucherUploadRs_Type">
		<xs:annotation>
			<xs:documentation>-an acknowledgement to the VoucherUploadRq message. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Timestamp"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VoucherConfirmationRq_Type">
		<xs:annotation>
			<xs:documentation>-provides an voucher load summary report.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="AsyncRqUID"/>
			<xs:element ref="UploadDt"/>
			<xs:element ref="PrcDt"/>
			<xs:element ref="SuccessRecordCount"/>
			<xs:element ref="ErrorRecordCount"/>
			<xs:element ref="VoucherErrorInfo" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VoucherConfirmationRs_Type">
		<xs:annotation>
			<xs:documentation>-acknowledgement of receipt of the VoucherConfirmationRq message </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="AsyncRqUID"/>
			<xs:element ref="UploadDt"/>
			<xs:element ref="PrcDt"/>
			<xs:element ref="SuccessRecordCount"/>
			<xs:element ref="ErrorRecordCount"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Voucher_Type">
		<xs:annotation>
			<xs:documentation>This aggregate is simply a wrapper around the voucher information that is to be communicated to requesting party.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="ExpDt" minOccurs="0"/>
			<xs:element ref="VoucherNumber"/>
			<xs:element ref="VoucherDesc" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- end of change-->
	<xs:complexType name="XferAddRq_Type">
		<xs:annotation>
			<xs:documentation>-move funds from Bank's SSA to either SCMA or SBA accounts </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="PrcDt"/>
			<xs:element ref="XferInfo" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="XferAddRs_Type">
		<xs:annotation>
			<xs:documentation>-confirm receipt of the intra-Bank transfer instructions</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="PrcDt"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="XferInfo_Type">
		<xs:annotation>
			<xs:documentation>-used in messages related to both Intrabank and Interbank Transfers</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="PmtId"/>
			<xs:element ref="CurAmt"/>
			<xs:element ref="DepAcctIdFrom"/>
			<xs:element ref="DepAcctIdTo"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="XferResultInfo_Type">
		<xs:annotation>
			<xs:documentation>-wrapper for the XferInfo aggregate, only it adds a Success element </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Status"/>
			<xs:element ref="XferInfo"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="XferResultRq_Type">
		<xs:annotation>
			<xs:documentation>-confirms the successful transfer of funds as directed by the XferAddRq message </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="PrcDt"/>
			<xs:element ref="XferResultInfo" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="XferResultRs_Type">
		<xs:annotation>
			<xs:documentation>-confirms SADAD has received the XferResultRq message </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="PrcDt"/>
		</xs:sequence>
	</xs:complexType>
