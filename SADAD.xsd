<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:complexType name="SADAD_Type">
		<xs:annotation>
			<xs:documentation>root element type</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:sequence>
				<xs:element ref="SignonRq"/>
				<xs:choice>
					<xs:element ref="BaseSvcRq"/>
					<xs:element ref="BankSvcRq"/>
					<xs:element ref="BillerSvcRq"/>
					<xs:element ref="PaySvcRq"/>
					<xs:element ref="PresSvcRq"/>
				</xs:choice>
				<xs:element ref="Signature" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:sequence>
				<xs:element ref="SignonRs"/>
				<xs:choice>
					<xs:element ref="BaseSvcRs"/>
					<xs:element ref="BankSvcRs"/>
					<xs:element ref="BillerSvcRs"/>
					<xs:element ref="PaySvcRs"/>
					<xs:element ref="PresSvcRs"/>
				</xs:choice>
				<xs:element ref="Signature" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:choice>
		<xs:attribute name="contentVersion" type="NC8_Type" default="2.0"/>
		<!--[17-April-2006][PD] Added communicationVersion in SADAD Tag as per Ext.Msg.Spec r2.0-->
	</xs:complexType>
	<xs:element name="SADAD" type="SADAD_Type">
		<xs:annotation>
			<xs:documentation>root element</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BankSvcRq_Type">
		<xs:annotation>
			<xs:documentation>-Bank Services are responsible for synchronization tasks performed exclusively with Banks</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Status" minOccurs="0"/>
			<xs:element ref="RqUID"/>
			<xs:choice>
				<xs:element ref="BankCutOffRq"/>
				<xs:element ref="BankReconDownloadRq"/>
				<xs:element ref="BankSplUploadRq"/>
				<xs:element ref="XferAddRq"/>
				<xs:element ref="XferResultRq"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BankSvcRs_Type">
		<xs:annotation>
			<xs:documentation>-Bank Services are responsible for synchronization tasks performed exclusively with Banks</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Status" minOccurs="0"/>
			<xs:element ref="RqUID"/>
			<xs:choice>
				<xs:element ref="BankCutOffRs"/>
				<xs:element ref="BankReconDownloadRs"/>
				<xs:element ref="BankSplUploadRs"/>
				<xs:element ref="XferAddRs"/>
				<xs:element ref="XferResultRs"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="BankSvcRq" type="BankSvcRq_Type"/>
	<xs:element name="BankSvcRs" type="BankSvcRs_Type"/>
	<xs:complexType name="BaseSvcRq_Type">
		<xs:annotation>
			<xs:documentation>-Base Services provide infrastructure to support ongoing SADAD operations</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Status" minOccurs="0"/>
			<xs:element ref="RqUID"/>
			<xs:choice>
				<xs:element ref="BillerInfoDownloadRq"/>
				<xs:element ref="CustPayeeAddRq"/>
				<xs:element ref="CustPayeeDelRq"/>
				<xs:element ref="CustPayeeInqRq"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BaseSvcRs_Type">
		<xs:annotation>
			<xs:documentation>-Base Services provide infrastructure to support ongoing SADAD operations</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Status" minOccurs="0"/>
			<xs:element ref="RqUID"/>
			<xs:choice>
				<xs:element ref="BillerInfoDownloadRs"/>
				<xs:element ref="CustPayeeAddRs"/>
				<xs:element ref="CustPayeeDelRs"/>
				<xs:element ref="CustPayeeInqRs"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="BaseSvcRq" type="BaseSvcRq_Type"/>
	<xs:element name="BaseSvcRs" type="BaseSvcRs_Type"/>
	<xs:complexType name="BillerSvcRq_Type">
		<xs:annotation>
			<xs:documentation>-Biller Services are responsible for synchronization tasks performed exclusively with Billers</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Status" minOccurs="0"/>
			<xs:element ref="RqUID"/>
			<xs:choice>
				<xs:element ref="AccountConfirmationRq"/>
				<xs:element ref="AccountUploadRq"/>
				<xs:element ref="BillerReconUploadRq"/>
				<xs:element ref="BillerTransLogUploadRq"/>
				<xs:element ref="PmtConfirmationRq"/>
				<xs:element ref="PmtDownloadRq"/>
				<xs:element ref="PmtLoadRq"/>
				<xs:element ref="PmtNotifyRq"/>
				<xs:element ref="PmtNotifyUploadRq"/>
				<xs:element ref="PmtUploadRq"/>
				<!--[17-April-2006][PD] Added VoucherUploadRq,VoucherConfirmationRq  element as per Ext.Msg.Spec r2.0 -->
				<xs:element ref="VoucherUploadRq"/>
				<xs:element ref="VoucherConfirmationRq"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BillerSvcRs_Type">
		<xs:annotation>
			<xs:documentation>-Biller Services are responsible for synchronization tasks performed exclusively with Billers</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Status" minOccurs="0"/>
			<xs:element ref="RqUID"/>
			<xs:choice>
				<xs:element ref="AccountConfirmationRs"/>
				<xs:element ref="AccountUploadRs"/>
				<xs:element ref="BillerReconUploadRs"/>
				<xs:element ref="BillerTransLogUploadRs"/>
				<xs:element ref="PmtConfirmationRs"/>
				<xs:element ref="PmtDownloadRs"/>
				<xs:element ref="PmtLoadRs"/>
				<xs:element ref="PmtNotifyRs"/>
				<xs:element ref="PmtNotifyUploadRs"/>
				<xs:element ref="PmtUploadRs"/>
				<!--[17-April-2006][PD] Added VoucherUploadRs,VoucherConfirmationRs element as per Ext.Msg.Spec r2.0 -->
				<xs:element ref="VoucherUploadRs"/>
				<xs:element ref="VoucherConfirmationRs"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="BillerSvcRq" type="BillerSvcRq_Type"/>
	<xs:element name="BillerSvcRs" type="BillerSvcRs_Type"/>
	<xs:complexType name="PaySvcRq_Type">
		<xs:annotation>
			<xs:documentation>-Payment Services involve the processes of accepting Customer funds </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Status" minOccurs="0"/>
			<xs:element ref="RqUID"/>
			<xs:choice>
				<xs:element ref="PmtAdviceRq"/>
				<xs:element ref="PmtModRq"/>
				<xs:element ref="PmtValidationRq"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PaySvcRs_Type">
		<xs:annotation>
			<xs:documentation>-Payment Services involve the processes of accepting Customer funds </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Status" minOccurs="0"/>
			<xs:element ref="RqUID"/>
			<xs:choice>
				<xs:element ref="PmtAdviceRs"/>
				<xs:element ref="PmtModRs"/>
				<xs:element ref="PmtValidationRs"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="PaySvcRq" type="PaySvcRq_Type"/>
	<xs:element name="PaySvcRs" type="PaySvcRs_Type"/>
	<xs:complexType name="PresSvcRq_Type">
		<xs:annotation>
			<xs:documentation>The presentment of both bill and payment data </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Status" minOccurs="0"/>
			<xs:element ref="RqUID"/>
			<xs:choice>
				<xs:element ref="BillConfirmationRq"/>
				<xs:element ref="BillDownloadRq"/>
				<xs:element ref="BillInqRq"/>
				<xs:element ref="BillUploadRq"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PresSvcRs_Type">
		<xs:annotation>
			<xs:documentation>The presentment of both bill and payment data </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Status" minOccurs="0"/>
			<xs:element ref="RqUID"/>
			<xs:choice>
				<xs:element ref="BillConfirmationRs"/>
				<xs:element ref="BillDownloadRs"/>
				<xs:element ref="BillInqRs"/>
				<xs:element ref="BillUploadRs"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="PresSvcRq" type="PresSvcRq_Type"/>
	<xs:element name="PresSvcRs" type="PresSvcRs_Type"/>
	<xs:simpleType name="AccessChannel_Enum">
		<xs:annotation>
			<xs:documentation>Defines the known values for the Open Enum AccessChannel</xs:documentation>
			<!--[10-Aug-2005][PD]:Modified AccessChannel Enum simpleType from as per Ext.Msg.Spec r1.7.1?-->
		</xs:annotation>
		<xs:restriction base="NC32_Type">
			<xs:enumeration value="ATM"/>
			<xs:enumeration value="IVR"/>
			<xs:enumeration value="KIOSK"/>
			<xs:enumeration value="INTERNET"/>
			<xs:enumeration value="PORTAL"/>
			<xs:enumeration value="BTELLER"/>
			<xs:enumeration value="POS"/>
			<xs:enumeration value="CAM"/>
			<xs:enumeration value="CORP"/>
			<xs:enumeration value="CCC"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccessChannel_Type">
		<xs:annotation>
			<xs:documentation>AccessChannel enum represents the tool in which humans interact with SADAD</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="AccessChannel_Enum NC32_Type"/>
	</xs:simpleType>
	<xs:simpleType name="AccountStatusCode_Type">
		<xs:annotation>
			<xs:documentation>Closed Enum that identifies an account?s status within SADAD or Biller systems</xs:documentation>
		</xs:annotation>
		<xs:restriction base="NC32_Type">
			<xs:enumeration value="AcctNew"/>
			<xs:enumeration value="AcctActivate"/>
			<xs:enumeration value="AcctDeact"/>
			<!--[10-Aug-2005][PD]:Removed AcctCancel fromAccountStatusCode enum simpleType as per Ext.Msg.Spec r1.7.1h?
			<xs:enumeration value="AcctCancel"/> Bug# 75 -->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AcctType_Enums">
		<xs:annotation>
			<xs:documentation>Defines the known values for the open enum AcctType</xs:documentation>
		</xs:annotation>
		<xs:restriction base="NC32_Type">
			<xs:enumeration value="CUA"/>
			<xs:enumeration value="CMA"/>
			<xs:enumeration value="DDA"/>
			<xs:enumeration value="LOC"/>
			<xs:enumeration value="MMA"/>
			<xs:enumeration value="SDA"/>
			<xs:enumeration value="SSA"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AcctType_Type">
		<xs:annotation>
			<xs:documentation>Open Enum for bank account types.</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="AcctType_Enums NC32_Type"/>
	</xs:simpleType>
	<xs:simpleType name="Amount_Type">
		<xs:annotation>
			<xs:documentation>An amount, usualy monetary.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="BillCategory_Enum">
		<xs:annotation>
			<xs:documentation>Defines the known values for the Open Enum BillCategory</xs:documentation>
		</xs:annotation>
		<xs:restriction base="NC12_Type">
			<xs:enumeration value="Recurring"/>
			<xs:enumeration value="CIP"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BillCategory_Type">
		<xs:annotation>
			<xs:documentation>A bill category assigns behaviour to a bill, thus permitting SADAD to apply rules to bills in varying manner.</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="BillCategory_Enum NC12_Type"/>
	</xs:simpleType>
	<xs:simpleType name="BillPmtStatusCode_Type">
		<xs:annotation>
			<xs:documentation>todo comment</xs:documentation>
		</xs:annotation>
		<xs:restriction base="NC32_Type"/>
	</xs:simpleType>
	<xs:simpleType name="BillStatusCode_Type">
		<xs:annotation>
			<xs:documentation>Closed Enum that identifies a bill?s payment status within SADAD</xs:documentation>
		</xs:annotation>
		<xs:restriction base="NC32_Type">
			<xs:enumeration value="BillPaid"/>
			<xs:enumeration value="BillPartialPd"/>
			<xs:enumeration value="BillOverPd"/>
			<xs:enumeration value="BillUnpaid"/>
			<xs:enumeration value="BillDeactive"/>
			<xs:enumeration value="BillNew"/>
			<xs:enumeration value="BillUpdated"/>
			<xs:enumeration value="BillAny"/>
			<xs:enumeration value="BillExpired"/>
			<xs:enumeration value="BillCreate"/>
			<xs:enumeration value="AcctAct"/>
			<xs:enumeration value="AcctDeact"/>
			<!--[28-Sep-2005][PD]:Added BillDeactive from BillStatusCode  enum simpleType as per Ext.Msg.Spec r1.7.2-->
			<!--[28-Jun-2005][PD]:Modified BillStatusCode enumeration as per Ext.Msg.Spec r1.7.1c. -->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BillSummAmtCode_Enum">
		<xs:annotation>
			<xs:documentation>Defines the known values for the Open Enum BillSummAmtCode</xs:documentation>
		</xs:annotation>
		<xs:restriction base="NC32_Type">
			<xs:enumeration value="Incentive"/>
			<xs:enumeration value="Penalty"/>
			<xs:enumeration value="MinPartialPmt"/>
			<xs:enumeration value="MinAdvancePmt"/>
			<xs:enumeration value="MaxAdvancePmt"/>
			<!-- [26-Jun-2005][PD]:Modified BillSummAmtCode enumeration as per Ext.Msg.Spec r1.7.1c. -->
			<!-- Deleted <xs:enumeration value="CreditLimit"/>-->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BillSummAmtCode_Type">
		<xs:annotation>
			<xs:documentation>Open Enum: Indicates the type of amount being specified in CurAmt</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="NC32_Type BillSummAmtCode_Enum"/>
	</xs:simpleType>
	<xs:simpleType name="BillSummAmtType_Enum">
		<xs:annotation>
			<xs:documentation>Indicates the type of amount specified in an associated currency value</xs:documentation>
		</xs:annotation>
		<xs:restriction base="NC32_Type">
			<xs:enumeration value="Payable"/>
			<xs:enumeration value="Supplemental"/>
			<xs:enumeration value="InfoOnly"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BillSummAmtType_Type">
		<xs:annotation>
			<xs:documentation>Open Enum: Indicates the type of amount specified in an associated currency value</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="NC32_Type BillSummAmtType_Enum"/>
	</xs:simpleType>
	<xs:simpleType name="C32_Type">
		<xs:annotation>
			<xs:documentation>32 character Unicode string</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="32"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="C40_Type">
		<xs:annotation>
			<xs:documentation>40 character Unicode string</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="40"/>
		</xs:restriction>
	</xs:simpleType>
	<!--	[17-April-2006][PD] Added C64_Type simpleType as per Ext.Msg.Spec r2.0-->
	<xs:simpleType name="C64_Type">
